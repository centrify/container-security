[centrify-rpm-redhat]
name=centrify-rpm-redhat
baseurl=https://cloudrepo.centrify.com/$CENTRIFY_REPOSITORY_KEY/rpm-redhat/rpm/any-distro/any-version/$basearch/
repo_gpgcheck=1
enabled=1
gpgkey=https://downloads.centrify.com/products/RPM-GPG-KEY-centrify
gpgcheck=1
sslverify=1
sslcacert=/etc/pki/tls/certs/ca-bundle.crt
metadata_expire=300
pkg_gpgcheck=1
autorefresh=1
type=rpm-md
